<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="httpPlatformHandler" path="*" verb="*" modules="httpPlatformHandler" resourceType="Unspecified" requireAccess="Script" />
    </handlers>
    <httpPlatform 
      stdoutLogEnabled="true" 
      stdoutLogFile=".\ErrorLog\python.log" 
      startupTimeLimit="20" 
      processPath="C:\Users\Jeet\AppData\Local\Programs\Python\Python314\python.exe" 
      arguments="-m uvicorn webapp.main:app --host 0.0.0.0 --port %HTTP_PLATFORM_PORT%">
      <environmentVariables>
        <environmentVariable name="PYTHONPATH" value="d:\JEET\Project\Python\MyApp" />
      </environmentVariables>
    </httpPlatform>
  </system.webServer>
</configuration>